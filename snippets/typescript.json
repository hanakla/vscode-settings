{
  /*
	 // Place your snippets for TypeScript here. Each snippet is defined under a snippet name and has a prefix, body and
	 // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	 // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	 // same ids are connected.
	 // Example:
	 "Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
  "declare module": {
    "prefix": "dm",
    "body": ["declare module '$1' {", "\t$2", "}"]
  },
  "extends": {
    "prefix": "et",
    "body": "extends"
  },
  "console.log": {
    "prefix": "csl",
    "body": "console.log($1)"
  },
  "console.erorr": {
    "prefix": "cse",
    "body": "console.error($1)"
  },
  "console.info": {
    "prefix": "csi",
    "body": "console.info($1)"
  },
  "console.trace": {
    "prefix": "cstr",
    "body": "console.trace($1)"
  },
  "console.time": {
    "prefix": "cst",
    "body": "console.time($1)\nconsole.timeEnd($1)"
  },
  "class": {
    "prefix": "cls",
    "body": ["class $1 {", "\t$2", "}"],
    "description": "description"
  },
  "import": {
    "prefix": "imp",
    "body": "import $1 from '$2'"
  },
  "export": {
    "prefix": "ex",
    "body": "export "
  },
  "export def": {
    "prefix": "ed",
    "body": "export default"
  },
  "export def func": {
    "prefix": "edf",
    "body": ["export default ${1:async }function $2($3) {", "\t$4", "}"]
  },
  "export def class": {
    "prefix": "edc",
    "body": ["export default class $1 {", "\t$2", "}"],
    "description": "description"
  },
  "export func": {
    "prefix": "ef",
    "body": ["export ${1:async }function $2($3) {", "\t$4", "}"]
  },
  "debugger": {
    "prefix": "dbg",
    "body": "debugger"
  }
}
